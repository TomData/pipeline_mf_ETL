# Optional settings file override
MF_ETL_SETTINGS_FILE=configs/settings.yaml

# Project
MF_ETL_PROJECT__NAME=mf_etl
MF_ETL_PROJECT__ENV=dev

# Paths
MF_ETL_PATHS__RAW_ROOT=/media/tom/Hdd_240GB/data
MF_ETL_PATHS__DATA_ROOT=./data
MF_ETL_PATHS__BRONZE_ROOT=./data/bronze
MF_ETL_PATHS__SILVER_ROOT=./data/silver
MF_ETL_PATHS__GOLD_ROOT=./data/gold
MF_ETL_PATHS__ARTIFACTS_ROOT=./artifacts
MF_ETL_PATHS__LOGS_ROOT=./logs

# Precision
MF_ETL_PRECISION__BRONZE_FLOAT=float64
MF_ETL_PRECISION__SILVER_FLOAT=float32
MF_ETL_PRECISION__GOLD_FLOAT=float32

# Parquet
MF_ETL_PARQUET__COMPRESSION=zstd
MF_ETL_PARQUET__COMPRESSION_LEVEL=3
MF_ETL_PARQUET__STATISTICS=true

# Validation thresholds
MF_ETL_VALIDATION__SUSPICIOUS_RANGE_PCT_THRESHOLD=0.5
MF_ETL_VALIDATION__SUSPICIOUS_RETURN_PCT_THRESHOLD=0.3
MF_ETL_VALIDATION__GAP_DAYS_WARN_THRESHOLD=7
